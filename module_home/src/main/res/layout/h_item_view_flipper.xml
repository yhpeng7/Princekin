    <com.yhklsdf.lib_common.ui.MyTextView
        android:id="@+id/h_tv_2"
        android:layout_width="30dp"
        android:layout_height="15dp"
        android:background="@drawable/h_tv_bg_1"
        android:textColor="#FF0000"
