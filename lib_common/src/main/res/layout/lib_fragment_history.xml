<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="热门搜索"
        android:textColor="@color/lib_color_search_tip"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="历史记录"
        android:textColor="@color/lib_color_search_tip"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <LinearLayout
        android:id="@+id/lib_linear_history_hot_containner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView">

        <include
            android:id="@+id/lib_search_hot_0"
            layout="@layout/lib_item_history_hot"
            android:visibility="gone" />

        <include
            android:id="@+id/lib_search_hot_1"
            layout="@layout/lib_item_history_hot"
            android:visibility="gone" />

        <include
            android:id="@+id/lib_search_hot_2"
            layout="@layout/lib_item_history_hot"
            android:visibility="gone" />

        <include
            android:id="@+id/lib_search_hot_3"
            layout="@layout/lib_item_history_hot"
            android:visibility="gone" />

        <include
            android:id="@+id/lib_search_hot_4"
            layout="@layout/lib_item_history_hot"
            android:visibility="gone" />


    </LinearLayout>

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50121653" />

    <ImageView
        android:id="@+id/lib_imgv_search_history_divider"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="#EAEAEA"
        app:layout_constraintBottom_toBottomOf="@+id/lib_txtv_search_history_9"
        app:layout_constraintEnd_toStartOf="@+id/lib_txtv_search_history_6"
        app:layout_constraintStart_toEndOf="@+id/lib_txtv_search_history_5"
        app:layout_constraintTop_toTopOf="@+id/lib_txtv_search_history_1" />

    <TextView

        android:id="@+id/lib_txtv_search_history_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="8dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/lib_txtv_search_history_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="8dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/lib_txtv_search_history_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/lib_txtv_search_history_1"
        app:layout_constraintTop_toBottomOf="@+id/lib_txtv_search_history_1" />

    <TextView
        android:id="@+id/lib_txtv_search_history_5"
